#include <stdio.h>
int max(int a,int b,int c);
void main()
{
    int a,b,c,big;
    printf("enter");
    scanf("%d %d %d",&a,&b,&c);
    
    big=max(a,b,c);
    printf("%d",big);
    
    return ;
}
int max(int a,int b, int c)
{
    int big;
    big=a;
    
    if(b>big)big=b;
    if(c>big)big=c;
    
    return big;
}
